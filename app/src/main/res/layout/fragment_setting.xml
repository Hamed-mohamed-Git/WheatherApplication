<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/frameLayout5"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.setting.SettingFragment">

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:text="@string/settings"
            android:textColor="@color/black"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guideline14" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/search_cardView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="32dp"
            android:background="@drawable/search_background"
            android:padding="7dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView12">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:background="@drawable/outline_search_24"
                android:backgroundTint="@color/gray"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView111"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:text="@string/search"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imageView4"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <ScrollView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="32dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/search_cardView">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <TextView
                    android:id="@+id/textView122"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="44dp"
                    android:text="@string/weather_settings"
                    android:textColor="@color/black"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/settingsCardView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="32dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="32dp"
                    app:cardCornerRadius="20dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView122">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">


                        <TextView
                            android:id="@+id/editListButton"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/setting_background"
                            android:paddingStart="24dp"
                            android:paddingTop="16dp"
                            android:paddingBottom="8dp"
                            android:text="@string/googlemap"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            tools:ignore="RtlSymmetry" />

                        <View
                            android:id="@+id/view7"
                            android:layout_width="0dp"
                            android:layout_height="0.6dp"
                            android:layout_marginStart="24dp"
                            android:background="@color/light_gray"
                            android:elevation="0.1dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/editListButton" />

                        <TextView
                            android:id="@+id/location"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/setting_background"
                            android:paddingStart="24dp"
                            android:paddingTop="8dp"
                            android:paddingBottom="16dp"
                            android:text="@string/location"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/view7"
                            tools:ignore="RtlSymmetry" />

                        <View
                            android:id="@+id/view9"
                            android:layout_width="0dp"
                            android:layout_height="15dp"
                            android:background="@color/setting_color"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/location" />

                        <TextView
                            android:id="@+id/kilometerSwitch"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:background="@drawable/setting_background"
                            android:paddingVertical="8dp"
                            android:paddingStart="24dp"
                            android:text="@string/kilometer"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/view9"
                            tools:ignore="RtlSymmetry" />

                        <View
                            android:id="@+id/view31"
                            android:layout_width="0dp"
                            android:layout_height="0.6dp"
                            android:layout_marginStart="24dp"
                            android:background="@color/light_gray"
                            android:elevation="0.1dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/kilometerSwitch" />


                        <TextView
                            android:id="@+id/mileSwitch"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/setting_background"
                            android:paddingStart="24dp"
                            android:paddingTop="8dp"
                            android:paddingBottom="16dp"
                            android:text="@string/mile"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/view31"
                            tools:ignore="RtlSymmetry" />

                        <View
                            android:id="@+id/view4"
                            android:layout_width="0dp"
                            android:layout_height="15dp"
                            android:background="@color/setting_color"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/mileSwitch" />

                        <TextView
                            android:id="@+id/notificationButton"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:background="@drawable/setting_background"
                            android:paddingVertical="8dp"
                            android:paddingStart="24dp"
                            android:text="@string/celsius"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/view4"
                            tools:ignore="RtlSymmetry" />

                        <View
                            android:id="@+id/view3"
                            android:layout_width="0dp"
                            android:layout_height="0.6dp"
                            android:layout_marginStart="24dp"
                            android:background="@color/light_gray"
                            android:elevation="0.1dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/notificationButton" />

                        <TextView
                            android:id="@+id/settingsButton"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/setting_background"
                            android:paddingVertical="8dp"
                            android:paddingStart="24dp"
                            android:text="@string/fahrenheit"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/notificationButton"
                            tools:ignore="RtlSymmetry" />

                        <View
                            android:id="@+id/view6"
                            android:layout_width="0dp"
                            android:layout_height="0.6dp"
                            android:layout_marginStart="24dp"
                            android:background="@color/light_gray"
                            android:elevation="0.1dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/settingsButton" />

                        <TextView
                            android:id="@+id/kelvin_button"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/setting_background"
                            android:paddingVertical="8dp"
                            android:paddingStart="24dp"
                            android:text="@string/kelvin"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/view6"
                            tools:ignore="RtlSymmetry" />

                        <View
                            android:id="@+id/view5"
                            android:layout_width="0dp"
                            android:layout_height="15dp"
                            android:layout_marginTop="8dp"
                            android:background="@color/setting_color"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/kelvin_button" />

                        <TextView
                            android:id="@+id/reportButton"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="10dp"
                            android:background="@drawable/setting_background"
                            android:paddingStart="24dp"
                            android:paddingTop="16dp"
                            android:paddingBottom="8dp"
                            android:text="@string/arabic"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/view5"
                            tools:ignore="RtlSymmetry" />

                        <View
                            android:id="@+id/view8"
                            android:layout_width="0dp"
                            android:layout_height="0.6dp"
                            android:layout_marginStart="24dp"
                            android:background="@color/light_gray"
                            android:elevation="0.1dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/reportButton" />


                        <TextView
                            android:id="@+id/aboutButton"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/setting_background"
                            android:paddingStart="24dp"
                            android:paddingTop="8dp"
                            android:paddingBottom="16dp"
                            android:text="@string/english"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/view8"
                            tools:ignore="RtlSymmetry" />


                        <Switch
                            android:id="@+id/locationSwitch"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="16dp"
                            android:thumb="@drawable/custom_thumb"
                            android:track="@drawable/custom_track"
                            app:layout_constraintBottom_toBottomOf="@+id/location"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/editListButton"
                            tools:ignore="UseSwitchCompatOrMaterialXml" />

                        <Switch
                            android:id="@+id/KiloSwitch"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="16dp"
                            android:thumb="@drawable/custom_thumb"

                            android:track="@drawable/custom_track"
                            app:layout_constraintBottom_toBottomOf="@+id/kilometerSwitch"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/view9"
                            tools:ignore="UseSwitchCompatOrMaterialXml" />

                        <Switch
                            android:id="@+id/MileSwitch"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="16dp"
                            android:thumb="@drawable/custom_thumb"
                            android:track="@drawable/custom_track"
                            app:layout_constraintBottom_toBottomOf="@+id/mileSwitch"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/kilometerSwitch"
                            tools:ignore="UseSwitchCompatOrMaterialXml" />

                        <Switch
                            android:id="@+id/googleMapSwitch"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="16dp"
                            android:thumb="@drawable/custom_thumb"
                            android:track="@drawable/custom_track"
                            app:layout_constraintBottom_toBottomOf="@+id/editListButton"
                            app:layout_constraintEnd_toEndOf="@+id/editListButton"
                            app:layout_constraintTop_toTopOf="@+id/editListButton"
                            app:layout_constraintVertical_bias="0.695"
                            tools:ignore="UseSwitchCompatOrMaterialXml" />

                        <Switch
                            android:id="@+id/celSwitch"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="16dp"
                            android:thumb="@drawable/custom_thumb"
                            android:track="@drawable/custom_track"
                            app:layout_constraintBottom_toBottomOf="@+id/notificationButton"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/view4"
                            tools:ignore="UseSwitchCompatOrMaterialXml" />

                        <Switch
                            android:id="@+id/fahrenheitSwitch"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="16dp"
                            android:thumb="@drawable/custom_thumb"
                            android:track="@drawable/custom_track"
                            app:layout_constraintBottom_toBottomOf="@+id/settingsButton"
                            app:layout_constraintEnd_toEndOf="@+id/settingsButton"
                            app:layout_constraintTop_toBottomOf="@+id/notificationButton"
                            tools:ignore="UseSwitchCompatOrMaterialXml" />

                        <Switch
                            android:id="@+id/kelvinSwitch"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="16dp"
                            android:thumb="@drawable/custom_thumb"
                            android:track="@drawable/custom_track"
                            app:layout_constraintBottom_toBottomOf="@+id/kelvin_button"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/settingsButton"
                            tools:ignore="UseSwitchCompatOrMaterialXml" />

                        <Switch
                            android:id="@+id/arabicSwitch"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="16dp"
                            android:thumb="@drawable/custom_thumb"

                            android:track="@drawable/custom_track"
                            app:layout_constraintBottom_toBottomOf="@+id/reportButton"
                            app:layout_constraintEnd_toEndOf="@+id/reportButton"
                            app:layout_constraintTop_toBottomOf="@+id/view5"
                            tools:ignore="UseSwitchCompatOrMaterialXml" />

                        <Switch
                            android:id="@+id/englishSwitch"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="18dp"
                            android:thumb="@drawable/custom_thumb"
                            android:track="@drawable/custom_track"
                            app:layout_constraintBottom_toBottomOf="@+id/aboutButton"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/reportButton"
                            tools:ignore="UseSwitchCompatOrMaterialXml" />


                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>

                <TextView
                    android:id="@+id/textView24"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="24dp"
                    android:text="@string/_2023_weather_com_all_rights_reserved"
                    android:textColor="@color/light_gray"
                    android:textSize="12sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/textView25"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="32dp"
                    android:layout_marginBottom="8dp"
                    android:text="@string/version_v1_00_1535"
                    android:textColor="@color/light_gray"
                    android:textSize="12sp"
                    app:layout_constraintBottom_toTopOf="@+id/textView24"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imageView10" />

                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="40dp"
                    android:layout_marginEnd="16dp"
                    android:background="@drawable/facebook"
                    android:backgroundTint="@color/light_gray"
                    app:layout_constraintEnd_toStartOf="@+id/imageView10"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintHorizontal_chainStyle="packed"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/settingsCardView" />

                <ImageView
                    android:id="@+id/imageView10"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="40dp"
                    android:layout_marginEnd="16dp"
                    android:background="@drawable/twitter"
                    android:backgroundTint="@color/light_gray"
                    app:layout_constraintEnd_toStartOf="@+id/imageView11"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/imageView9"
                    app:layout_constraintTop_toBottomOf="@+id/settingsCardView" />

                <ImageView
                    android:id="@+id/imageView11"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="40dp"
                    android:layout_marginEnd="16dp"
                    android:background="@drawable/instagram"
                    android:backgroundTint="@color/light_gray"
                    app:layout_constraintEnd_toStartOf="@+id/imageView12"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/imageView10"
                    app:layout_constraintTop_toBottomOf="@+id/settingsCardView" />

                <ImageView
                    android:id="@+id/imageView12"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="40dp"
                    android:layout_marginEnd="16dp"
                    android:background="@drawable/linkedin"
                    android:backgroundTint="@color/light_gray"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/imageView11"
                    app:layout_constraintTop_toBottomOf="@+id/settingsCardView" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </ScrollView>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.06" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>